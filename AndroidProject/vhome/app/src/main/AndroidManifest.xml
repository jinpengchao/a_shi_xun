<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="h.jpc.vhome">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".parents.fragment.alarm.AlarmActivity" />
        <activity android:name=".parents.fragment.tools.AllToolsActivity" />
        <activity android:name=".children.ChildrenMain" />
        <activity android:name=".parents.ParentMain" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".children.fragment.WarnFragment" />
    </application>

</manifest>